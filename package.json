{
    "activationEvents": [
        "*"
    ],
    "author": {
        "name": "Simon Chan",
        "url": "https://github.com/yume-chan"
    },
    "bugs": {
        "url": "https://github.com/yume-chan/vscode-svn/issues"
    },
    "categories": [
        "SCM Providers"
    ],
    "contributes": {
        "commands": [
            {
                "category": "Svn",
                "command": "svn.update",
                "title": "%command.update%"
            },
            {
                "category": "Svn",
                "command": "svn.commit",
                "icon": {
                    "dark": "resources/icons/dark/check.svg",
                    "light": "resources/icons/light/check.svg"
                },
                "title": "%command.commit%"
            },
            {
                "category": "Svn",
                "command": "svn.stage",
                "icon": {
                    "dark": "resources/icons/dark/stage.svg",
                    "light": "resources/icons/light/stage.svg"
                },
                "title": "%command.stage%"
            },
            {
                "category": "Svn",
                "command": "svn.stageAll",
                "icon": {
                    "dark": "resources/icons/dark/stage.svg",
                    "light": "resources/icons/light/stage.svg"
                },
                "title": "%command.stageAll%"
            },
            {
                "category": "Svn",
                "command": "svn.unstage",
                "icon": {
                    "dark": "resources/icons/dark/unstage.svg",
                    "light": "resources/icons/light/unstage.svg"
                },
                "title": "%command.unstage%"
            },
            {
                "category": "Svn",
                "command": "svn.unstageAll",
                "icon": {
                    "dark": "resources/icons/dark/unstage.svg",
                    "light": "resources/icons/light/unstage.svg"
                },
                "title": "%command.unstageAll%"
            },
            {
                "category": "Svn",
                "command": "svn.update",
                "title": "%command.update%"
            },
            {
                "category": "Svn",
                "command": "svn.refresh",
                "icon": {
                    "dark": "resources/icons/dark/refresh.svg",
                    "light": "resources/icons/light/refresh.svg"
                },
                "title": "%command.refresh%"
            },
            {
                "category": "Svn",
                "command": "svn.openFile",
                "icon": {
                    "dark": "resources/icons/dark/open-file.svg",
                    "light": "resources/icons/light/open-file.svg"
                },
                "title": "%command.openFile%"
            }
        ],
        "configuration": {
            "properties": {
                "svn.show_changes_from": {
                    "default": "working_copy",
                    "description": "%config.show_changes_from%",
                    "enum": [
                        "current_folder",
                        "working_copy"
                    ],
                    "scope": "window",
                    "type": "string"
                }
            },
            "title": "Svn"
        },
        "menus": {
            "scm/resourceGroup/context": [
                {
                    "command": "svn.stageAll",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.stageAll",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.unstageAll",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "svn.unstageAll",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                }
            ],
            "scm/resourceState/context": [
                {
                    "command": "svn.stage",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.stage",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup != staged"
                },
                {
                    "command": "svn.unstage",
                    "group": "1_modification",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "svn.unstage",
                    "group": "inline",
                    "when": "scmProvider == svn && scmResourceGroup == staged"
                },
                {
                    "command": "git.openFile",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                }
            ],
            "scm/title": [
                {
                    "command": "svn.refresh",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                },
                {
                    "command": "svn.commit",
                    "group": "navigation",
                    "when": "scmProvider == svn"
                }
            ]
        }
    },
    "dependencies": {
        "node-svn": "git+https://github.com/yume-chan/node-svn.git"
    },
    "description": "An experimental extension to provide SVN support for Visual Studio Code.",
    "devDependencies": {
        "@types/mocha": "^2.2.44",
        "@types/node": "^8.0.53",
        "mocha": "^4.0.1",
        "tslint": "^5.8.0",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.6.1",
        "vscode": "^1.1.8"
    },
    "displayName": "Svn",
    "engines": {
        "vscode": "^1.18.0"
    },
    "license": "MIT",
    "main": "./out/extension",
    "name": "vscode-svn",
    "preview": true,
    "publisher": "simonchan",
    "repository": {
        "type": "git",
        "url": "https://github.com/yume-chan/vscode-svn"
    },
    "scripts": {
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "vscode:prepublish": "npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "version": "0.0.7"
}