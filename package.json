{
    "activationEvents": [
        "*"
    ],
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "category": "SVN",
                "command": "svn.update",
                "title": "Update"
            },
            {
                "category": "SVN",
                "command": "svn.commit",
                "title": "Commit"
            },
            {
                "category": "SVN",
                "command": "svn.stage",
                "title": "Stage"
            },
            {
                "category": "SVN",
                "command": "svn.unstage",
                "title": "Unstage"
            }
        ],
        "menus": {
            "scm/resourceState/context": [
                {
                    "command": "svn.stage",
                    "when": "scmProvider == svn && svnState == running"
                },
                {
                    "command": "svn.unstage",
                    "when": "scmProvider == svn && svnState == running"
                }
            ],
            "scm/title": [
                {
                    "command": "svn.commit",
                    "when": "scmProvider == svn && svnState == running"
                }
            ]
        }
    },
    "dependencies": {
        "fs-extra": "^4.0.2"
    },
    "description": "",
    "devDependencies": {
        "@types/fs-extra": "^4.0.2",
        "@types/mocha": "^2.2.43",
        "@types/node": "^8.0.28",
        "mocha": "^3.5.3",
        "tslint": "^5.7.0",
        "tslint-language-service": "^0.9.6",
        "typescript": "^2.5.2",
        "vscode": "^1.1.5"
    },
    "displayName": "SVN",
    "engines": {
        "vscode": "^1.16.0"
    },
    "license": "MIT",
    "main": "./out/extension",
    "name": "vscode-svn",
    "publisher": "cnsimonchan",
    "scripts": {
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "vscode:prepublish": "node-gyp -C svn rebuild --target=1.7.7 --arch=x64 --dist-url=https://atom.io/download/electron --debug && node svn/scripts/postinstall.js debug && tsc -p ./"
    },
    "version": "0.0.1"
}
